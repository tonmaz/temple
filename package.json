{
  "name": "temple",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "analyze": "BUNDLE_ANALYZE=both yarn build",
    "prettier-fix": "prettier --write .",
    "find:unused": "next-unused",
    "format": "prettier --write .",
    "lint:fix": "eslint . --ext ts --ext tsx --ext js --fix",
    "svg": "npx @svgr/cli -d src/common/components/elements/Icons --ignore-existing src/common/lib/svg-to-react/svgSource --ext tsx --template src/common/lib/svg-to-react/svgTemplate.ts",
    "test-all": "yarn lint && yarn type-check && yarn test",
    "packages:upgrade": "yarn upgrade-interactive --latest",
    "generate": "cross-env TS_NODE_PROJECT='./internals/ts-node.tsconfig.json' plop --plopfile internals/generators/plopfile.ts",
    "tdt": "npx devtools-server -c tailwind.config.js"
  },
  "next-unused": {
    "alias": {},
    "debug": true,
    "include": [
      "src"
    ],
    "exclude": [
      ".test.",
      "src/utils/extend-jest/*",
      "src/utils/svg/svgTemplate.ts"
    ],
    "entrypoints": [
      "src/pages"
    ]
  },
  "dependencies": {
    "@aws-amplify/ui-react": "1.2.1",
    "@babel/core": "7.14.3",
    "@headlessui/react": "1.2.0",
    "@heroicons/react": "1.0.1",
    "@popperjs/core": "2.9.2",
    "@react-page/editor": "^2.3.0",
    "@tiptap/core": "^2.0.0-beta.80",
    "@tiptap/extension-paragraph": "^2.0.0-beta.14",
    "@tiptap/extension-placeholder": "^2.0.0-beta.21",
    "@tiptap/react": "^2.0.0-beta.45",
    "@tiptap/starter-kit": "^2.0.0-beta.76",
    "@types/fontfaceobserver": "0.0.6",
    "@types/react-dom": "17.0.6",
    "@types/react-router-dom": "5.1.7",
    "@types/react-test-renderer": "17.0.1",
    "@types/rimraf": "3.0.0",
    "@types/shelljs": "0.8.8",
    "@types/testing-library__jest-dom": "5.9.5",
    "@types/webpack": "5.28.0",
    "@types/webpack-env": "1.16.0",
    "accounting": "0.4.1",
    "autoprefixer": "10.2.6",
    "aws-amplify": "4.0.3",
    "chalk": "4.1.1",
    "classnames": "2.3.1",
