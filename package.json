    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "faker": "5.5.3",
    "html-react-parser": "^1.2.6",
    "madge": "4.0.2",
    "moment": "2.29.1",
    "next": "10.2.3",
    "next-translate": "1.0.7",
    "node-plop": "0.26.2",
    "nprogress": "0.2.0",
